{
  "name": "daakiya-server",
  "version": "0.0.1",
  "description": "Daakiya server handles client message read requests and also handles push noti",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps package.json",
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir ./lib",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean ./lib",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"./lib/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs ./lib/__tests__/**/*.js && npm run posttest",
    "migrate": "node ./lib/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^1.0.12",
    "@loopback/context": "^1.5.1",
    "@loopback/core": "^1.1.6",
    "@loopback/openapi-v3": "^1.2.1",
    "@loopback/repository": "^1.1.5",
    "@loopback/rest": "^1.5.5",
    "@loopback/rest-explorer": "^1.1.8",
    "@loopback/service-proxy": "^1.0.8",
    "loopback-connector-sqlite3": "^1.2.0"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.0",
    "@loopback/testlab": "^1.0.6",
    "@loopback/tslint-config": "^2.0.0",
    "@types/node": "^10.11.2",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1",
    "electron": "^3.0.10",
    "electron-builder": "^20.34.0"
  },
  "resolutions": {
    "nan": "https://github.com/nodejs/nan"
  }
}
